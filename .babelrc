{
  "env": {
    "development": {
      "plugins": [
        "inline-dotenv",
        [
          "babel-plugin-styled-components",
          {
            "ssr": true,
            "displayName": true,
            "preprocess": false
          }
        ]
      ],
      "presets": ["next/babel"],
    },
    "production": {
      "plugins": [
        "transform-inline-environment-variables",
        [
          "babel-plugin-styled-components",
          {
            "ssr": true,
            "displayName": false,
            "preprocess": false
          }
        ]
      ],
      "presets": ["next/babel"],
    },
    "test": {
      "plugins": [
        "inline-dotenv",
        [
          "babel-plugin-styled-components",
          {
            "ssr": true,
            "displayName": true,
            "preprocess": false
          }
        ]
      ],
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]]
    }
  }
}
