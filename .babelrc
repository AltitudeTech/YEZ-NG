{
  "env": {
    "development": {
      "plugins": ["inline-dotenv", "styled-components"],
      "presets": ["next/babel"]
    },
    "production": {
      "plugins": ["transform-inline-environment-variables", "styled-components"],
      "presets": ["next/babel"]
    },
    "test": {
      "plugins": ["inline-dotenv", "styled-components"],
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]]
    }
  }
}
